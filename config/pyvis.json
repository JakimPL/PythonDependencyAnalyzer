{
    "pda": {
        "auto_adjust_spacing": true,
        "ratio": 8.0
    },
    "network": {
        "notebook": true,
        "height": "750px",
        "width": "100%",
        "cdn_resources": "remote"
    },
    "vis": {
        "edges": {
            "smooth": {
                "enabled": true,
                "type": "cubicBezier",
                "roundness": 0.5
            },
            "arrows": {
                "to": {
                    "enabled": true,
                    "scaleFactor": 0.5
                }
            }
        },
        "nodes": {
            "shape": "box",
            "font": {
                "size": 14,
                "face": "monospace"
            }
        },
        "groups": {
            "import": {
                "color": {
                    "background": "#BA68C8",
                    "border": "#8E24AA"
                }
            },
            "definition": {
                "color": {
                    "background": "#42A5F5",
                    "border": "#1E88E5"
                }
            },
            "control_flow": {
                "color": {
                    "background": "#FFA726",
                    "border": "#F57C00"
                }
            },
            "operator": {
                "color": {
                    "background": "#66BB6A",
                    "border": "#43A047"
                }
            },
            "name": {
                "color": {
                    "background": "#26C6DA",
                    "border": "#00ACC1"
                }
            },
            "assignment": {
                "color": {
                    "background": "#FFCA28",
                    "border": "#FFA000"
                }
            },
            "call": {
                "color": {
                    "background": "#9575CD",
                    "border": "#7E57C2"
                }
            },
            "data_structure": {
                "color": {
                    "background": "#29B6F6",
                    "border": "#039BE5"
                }
            },
            "control_transfer": {
                "color": {
                    "background": "#EC407A",
                    "border": "#D81B60"
                }
            },
            "expression": {
                "color": {
                    "background": "#BDBDBD",
                    "border": "#757575"
                }
            },
            "metadata": {
                "color": {
                    "background": "#78909C",
                    "border": "#546E7A"
                }
            },
            "declaration": {
                "color": {
                    "background": "#A1887F",
                    "border": "#6D4C41"
                }
            },
            "statement": {
                "color": {
                    "background": "#BDBDBD",
                    "border": "#757575"
                }
            }
        },
        "layout": {
            "hierarchical": {
                "enabled": true,
                "direction": "LR",
                "sortMethod": "directed",
                "nodeSpacing": 50,
                "levelSeparation": 250,
                "treeSpacing": 100,
                "blockShifting": false,
                "edgeMinimization": true,
                "parentCentralization": false
            }
        },
        "interaction": {
            "hover": true,
            "navigationButtons": true,
            "keyboard": true,
            "zoomView": true,
            "dragView": true
        },
        "physics": {
            "enabled": false
        }
    }
}